{
  "name": "privi-launchpad-be",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "start:dev": "nodemon dev",
    "start:devssl": "nodemon devssl",
    "start:prod": "nodemon prod",
    "lint": "npm run eslint && npm run typecheck",
    "eslint": "eslint --ext .ts,.tsx .",
    "typecheck": "tsc --noEmit",
    "format": "npm run format-code && npm run format-assets",
    "format-code": "eslint --fix --ext .ts,.js .",
    "format-assets": "prettier --write './**/*.{json,yml,yaml,md,html}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@compound-finance/compound-js": "^0.4.0",
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.0",
    "@types/node-cron": "^2.0.4",
    "@types/node-uuid": "0.0.29",
    "@types/sanitize-html": "^2.3.2",
    "axios": "^0.21.1",
    "axios-case-converter": "^0.8.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.11.0",
    "graphql": "^15.5.3",
    "graphql-request": "^3.5.0",
    "helmet": "^4.6.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "node-uuid": "^1.4.8",
    "nodemon": "^2.0.12",
    "opensea-js": "^1.2.0",
    "opensea-scraper": "^1.0.2",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "sanitize-html": "^2.4.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "web3": "^1.3.3"
  },
  "devDependencies": {
    "@types/express-rate-limit": "^5.1.3",
    "@types/redis": "^2.8.31",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "express-rate-limit": "^5.3.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --fix --cache",
    "*.{json,yml,yaml,md,html}": "prettier --write"
  }
}
